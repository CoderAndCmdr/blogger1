<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag) %>
    </li>
  <% end %>
</ul>

<% if logged_in? %>
  <%= link_to "delete", tag_path(@tag, method: :delete %>
  <%= link_to "Create a New Tag", new_tag_path, class: "new_tag" %>
<% end %>
